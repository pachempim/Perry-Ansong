#include <iostream>
using namespace std;
// prime is method defined to check whether a number is prime
bool prime(int n){
    int i,c=0;
    for(i=2;i<=n;i++)
    {
        if(n%i==0)
        c++;
    }
    if(c==1)
    return true;
    else;
    returns false;
}
int main()
{
    int i,s=0;
    for(i=1;i<=100;i++)
    {
        if(prime(i)==true)// here if the number is prime then add
        s+=i;
    }
    cout<<"Sum of Prime numbers between 1 to 100 : "<<s;
    return 0;
}
